<label><%-@title%></label>
<% if @help_text? : %>
  <span class="help-block">
    <%- @help_text %>
  </span>
<% end %>
<% for option in @entry_field_options.models: %>
  <label class="radio">
    <input type="radio" name="<%-"entry_field_#{option.get('id')}"%>" value="<%-"#{option.get('id')}"%>" id="<%-"single_choice_value_#{option.get('id')}"%>">
      <%= option.get("text_option") %>
  </label>
<% end %>
