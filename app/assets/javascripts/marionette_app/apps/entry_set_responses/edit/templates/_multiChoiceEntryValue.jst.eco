<label><%-@title%></label>
<% if @help_text? : %>
  <span class="help-block">
    <%- @help_text %>
  </span>
<% end %>

<% for option in @entry_field_options.models: %>
  <label class="checkbox">
    <input type="checkbox" name="<%-"multi_choice_value_#{@id}"%>" value="<%-"#{option.get('id')}"%>">
      <%= option.get("text_option") %>
  </label>
<% end %>
