	<div id="form-content-region"></div>
	<% if @footer: %>
			<% if @buttons: %>
				<p class="default-buttons">
					<% for button in @buttons: %>
						<button type="button" data-form-button="<%= button.buttonType %>" class="<%= button.className %>"><%= button.text %></button>
					<% end %>
				</p>
			<% end %>
	<% end %>

